{
  "name": "nostalgic",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "engines": {
    "node": "v16.13.0"
  },
  "scripts": {
    "start": "NODE_ENV=production node ./dist/server.js",
    "dev": "NODE_ENV=dev nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' -e ts",
    "build": "rm -rf ./dist && tsc -p tsconfig.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "test": "NODE_ENV=test yarn run jest --passWithNoTests --noStackTrace --runInBand --silent",
    "test:verbose": "NODE_ENV=test yarn run jest --passWithNoTests --noStackTrace --runInBand --verbose",
    "test:watch": "NODE_ENV=test yarn run jest --passWithNoTests --noStackTrace --runInBand --watch",
    "migration:run": "yarn run typeorm migration:run",
    "migration:generate": "NODE_ENV=development yarn run typeorm migration:generate -n ",
    "migration:create": "NODE_ENV=development yarn run typeorm migration:create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rabbithay/nostalgic-api.git"
  },
  "keywords": [],
  "author": "https://github.com/rabbithay",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rabbithay/nostalgic-api/issues"
  },
  "homepage": "https://github.com/rabbithay/nostalgic-api#readme",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/joi": "^17.2.3",
    "@types/node": "^16.11.12",
    "@types/pg": "^8.6.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "joi": "^17.5.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41",
    "typeorm-naming-strategies": "^2.0.0",
    "uuid": "^8.3.2"
  }
}
